{
  "name": "grant-predictor",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --incremental",
    "setup:db": "node scripts/setup-database.js",
    "deploy": "npm run build && vercel --prod",
    "deploy:test": "vercel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "security:audit": "npm audit --audit-level=moderate",
    "bundle-analyze": "ANALYZE=true next build",
    "pre-deploy": "npm run type-check && npm run lint && npm run test:ci",
    "postinstall": "echo 'Run npm run setup:db to initialize database'"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.28",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-tabs": "^1.1.2",
    "@react-three/drei": "^9.114.3",
    "@react-three/fiber": "^8.17.10",
    "@stripe/stripe-js": "^4.9.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.14.4",
    "lucide-react": "^0.469.0",
    "next": "15.4.5",
    "openai": "^5.12.0",
    "pg": "^8.16.3",
    "react": "18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.14.1",
    "stripe": "^17.4.0",
    "tailwind-merge": "^2.5.5",
    "three": "^0.170.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0.170.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^29.7.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
